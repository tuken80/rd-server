{
   "name": "mad-server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/express": "^4.17.11",
      "@types/node": "^8.0.29",
      "nodemon": "^2.0.7",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "bcrypt": "^5.0.1",
      "body-parser": "^1.18.1",
      "class-transformer": "^0.4.0",
      "class-validator": "^0.13.1",
      "dotenv": "^8.2.0",
      "express": "^4.15.4",
      "jose": "^3.11.0",
      "moment": "^2.29.1",
      "mysql": "^2.14.1",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.31",
      "winston": "^3.3.3"
